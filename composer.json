{
    "name": "contao-community-alliance/backports-initialize-system",
    "description": "Backport of the initializeSystem Hook for Contao Open Source CMS.",
    "keywords": [
        "contao",
        "backports"
    ],
    "type": "contao-module",
    "homepage": "http://c-c-a.org/",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Tristan Lins",
            "email": "tristan.lins@bit3.de",
            "homepage": "http://bit3.de",
            "role": "Developer"
        },
        {
            "name": "Ruben Roegels, wiseape GmbH",
            "email": "ruben.roegels@wiseape.de",
            "role": "Contributor"
        },
        {
            "name": "Andreas Isaak",
            "email": "andy.jared@googlemail.com",
            "role": "Contributor"
        }
    ],
    "support": {
        "email": "info@bit3.de",
        "issues": "https://github.com/contao-community-alliance/backports-initialize-system/issues",
        "wiki": "https://github.com/contao-community-alliance/backports-initialize-system/wiki",
        "irc": "irc://irc.freenode.org/contao.dev",
        "source": "https://github.com/contao-community-alliance/backports-initialize-system"
    },
    "require": {
        "php": ">=5.3",
        "contao/core": ">=2.11-dev,<4-dev",
        "contao-community-alliance/composer-plugin": "~2.0"
    },
    "require-dev": {
        "composer/composer": "dev-master",
        "contao-community-alliance/coding-standard": "~1.0",
        "contao-community-alliance/build-system": "~1.0",
        "contao-community-alliance/build-system-all-tasks": "~1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ContaoCommunityAlliance\\Contao\\Backports\\": "contao/src/"
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "contao": "system/modules/backports-initialize-system"
            }
        },
        "branch-alias": {
            "dev-master": "1.1.x-dev",
            "dev-develop": "1.2.x-dev"
        }
    }
}
